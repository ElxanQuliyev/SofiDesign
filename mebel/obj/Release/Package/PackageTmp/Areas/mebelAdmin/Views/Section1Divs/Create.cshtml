@model mebel.Models.section1Divs

@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/mebelAdmin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Create</h2>
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label class="control-label col-md-2">Header</label>
        <div class="col-md-10">
            <input class="form-control" type="text" name="Header" value="" />
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">Description</label>
        <div class="col-md-10">
            <textarea rows="7" class="form-control" type="text" name="Description" value=""></textarea>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">Photo</label>
        <div class="col-md-10">
            <input type="file" name="Image" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-2">
            <input type="submit" class="btn btn-success" value="Create" />
        </div>
    </div>
</form>

